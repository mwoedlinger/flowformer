{
    "name": "bue_bln_augmented_smaller",
    "n_gpu": 1,
    "arch": {
        "type": "SetTransformer",
        "args": {
            "num_inds": 16,
            "dim_hidden": 32,
            "num_heads": 4,
            "layer_norm": true,
            "hidden_layers": 3,
            "residual": false,
            "mode": "binary"
        }
    },
    "data_loader": {
        "type": "FlowDataLoader",
        "args": {
            "train": "data/bue_bln/train.txt",
            "eval": "data/bue_bln/eval.txt",
            "test": "data/bue_bln/test.txt",
            "perturb_all": 1,
            "perturb_blasts": 2,
            "augmentation_intensity": 0.5,
            "fast_preload_dir": "/data/mwoedlinger/data/tmp/bue_bln",
            "fast_preload": true,
            "markers": "TIME, FSC-A, FSC-W, SSC-A, CD20, CD10, CD45, CD34, CD19, CD38, SY41",
            "sequence_length": false,
            "batch_size": 1,
            "num_workers": 2
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "amsgrad": true
        }
    },
    "loss": "bce_loss",
    "metrics": [
        "f1_score",
        "mrd_gt",
        "mrd_pred",
        "precision",
        "recall"
    ],
    "lr_scheduler": {
        "type": "CosineAnnealingLR",
        "args": {
            "T_max": 30,
            "eta_min": 0.00002
        }
    },
    "trainer": {
        "epochs": 200,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 1,
        "monitor": "max val_f1_score",
        "visualize": [
            ["FSC-A", "SSC-A"], 
            ["CD45", "SSC-A"], 
            ["CD19", "SSC-A"], 
            ["CD45", "CD10"]
        ],
        "early_stop": 50,
        "logger": "wandb"
    }
}